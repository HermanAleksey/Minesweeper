<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>


    /**------------------------------------------------OLD---------------------------------------------------**/
    //Main activity OLD

    <style name="background_layout_old">
        <item name="android:background">@color/colorMainBackground_old</item>
    </style>

    <style name="game_label_old">
        <item name="fontFamily">@font/bungee_shade_regular</item>
        <item name="android:textSize">@dimen/main_label_text_size</item>
        <item name="android:paddingTop">@dimen/main_label_padding_top</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:paddingBottom">@dimen/main_label_padding_bottom</item>
    </style>

    <style name="main_button_old">
        <item name="android:background">@drawable/my_main_button</item>
        <item name="android:layout_margin">8dp</item>
        <item name="android:fontFamily">@font/inverserif_regular</item>
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">@color/colorButtonLabel_old</item>
        <item name="android:layout_gravity">center</item>
    </style>

    //Company levels

    <style name="main_label_old">
        <item name="fontFamily">@font/art_modern_ap</item>
        <item name="android:textSize">@dimen/company_label_text_size</item>
        <item name="android:layout_margin">@dimen/company_label_margin</item>
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    //Game settings

    <style name="strong_line_separator_old">
        <item name="android:background">@drawable/big_line_1</item>
    </style>

    <style name="secondary_horizontal_line_old">
        <item name="android:background">@drawable/small_line_1</item>
    </style>

    <style name="checkbox_game_settings_old">
        <item name="android:button">@drawable/my_checkbox</item>
    </style>

    //Result activity

    <style name="game_result_border_old">
        <item name="android:background">@drawable/background_border_1</item>
    </style>

    <style name="text_result_game_result_old">
        <item name="android:fontFamily">@font/art_modern_ap</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">@dimen/result_activity_label_text_size</item>
    </style>

    <style name="casual_description_old">
        <item name="android:layout_gravity">center</item>
        <item name="android:textColor">@color/colorButtonLabel_old</item>
        <item name="fontFamily">@font/inverserif_regular</item>
        <item name="android:textSize">@dimen/result_activity_description_text_size</item>
    </style>

    //Minefield activity

    <style name="minefield_time_label_old">
        <item name="android:fontFamily">@font/rectangle_numbers</item>
        <item name="android:textSize">@dimen/minefield_time_text_size</item>
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="secondary_vertical_line_old">
        <item name="android:background">@drawable/vertical_gradient_line</item>
    </style>

    <style name="minefield_toggle_button_old">
        <item name="android:layout_margin">@dimen/minefield_toggle_button_margin</item>
        <item name="android:background">@drawable/my_toggle_button</item>
        <item name="android:textOff">""</item>
        <item name="android:textOn">""</item>
    </style>

    <style name="minefield_filed_background_old">
        <item name="background">@color/colorPrimary</item>
    </style>

    <style name="seekBar_cell_size_old">
        <item name="android:buttonTint">@color/colorPrimaryDark</item>
    </style>

    <style name="helper_dialog_text_old"></style>



